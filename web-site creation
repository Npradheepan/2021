<script src="https://code.jquery.com/jquery-2.2.4.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js"></script>

<script src="https://cdn.jsdelivr.net/npm/nicescroll@3.7.4/jquery.nicescroll.min.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-countto/1.2.0/jquery.countTo.min.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/mixitup/3.2.2/mixitup.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/wow/1.1.2/wow.min.js"></script>

<script>
  var wow = new WOW();
  wow.init();
</script>
</body>
<script>
/*global $, alert, console  */
$(document).ready(function () {
  "use strict";
  // Slider Height

  // Page Loading
  $(window).on("load", function () {
    $(".loading .sk-cube-grid").fadeOut(1000, function () {
      $(this)
        .parent()
        .fadeOut(1000, function () {
          $("body").css("overflow", "auto");
          $(this).remove();
        });
    });
  });

  var winH = $(window).height();
  $(".carousel-item").height(winH);

  // Change Active
  $("nav .navbar-nav .nav-item").on("click", function () {
    $(this).addClass("active").siblings().removeClass("active");
  });

  $("nav ul li .home").on("click", function (e) {
    e.preventDefault();
    $("html, body").animate(
      {
        scrollTop: 0
      },
      1500
    );
  });
  // Go To The Section
  $("nav ul li a").on("click", function (e) {
    e.preventDefault();
    $("html, body").animate(
      {
        scrollTop: $($(this).data("target")).offset().top
      },
      1500
    );
  });

  // Go To Top
  $(window).scroll(function () {
    if ($(this).scrollTop() <= $(".about").offset().top - 250) {
      $(".top .fa").fadeOut();
    } else {
      $(".top .fa").fadeIn();
    }
  });

  $(".top .fa").on("click", function () {
    $("html, body").animate(
      {
        scrollTop: 0
      },
      1500
    );
  });

  // Trigger Count To
  $(window).scroll(function () {
    if ($(this).scrollTop() > $(".video").offset().top + 150) {
      $(".timer").countTo();
    }
  });

  $(".port .list-unstyled li").on("click", function () {
    $(this).addClass("active").siblings("li").removeClass("active");
  });

  // Trigger MixitUp Plugin
  window.mixitup("#shuffle");

  // Trigger NiceScroll
  $("body").niceScroll({
    cursorcolor: "#6AB571",
    cursorborder: "1px solid #6AB571",
    cursorwidth: "7px",
    cursorminheight: 40
  });

  $(".carousel").carousel({
    pause: "hover",
    interval: 3000
  });
});

</script>
</html>
